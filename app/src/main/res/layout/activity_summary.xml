                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:background="@drawable/bg_ring" />

                    <TextView
                        android:id="@+id/Assignment_1_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentBottom="true"
                        android:layout_centerHorizontal="true"
                        android:layout_marginBottom="19dp"
                        android:fontFamily="serif"
                        android:text="borrowed"
                        android:textAlignment="center"
                        android:textColor="@android:color/white"
                        android:textSize="18sp" />

                    <EditText
                        android:id="@+id/Assignment_1_number"
                        style="@style/Widget.AppCompat.AutoCompleteTextView"
                        android:layout_width="67dp"
                        android:layout_height="wrap_content"
                        android:layout_alignParentTop="true"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="15dp"
                        android:ems="10"
                        android:fontFamily="serif"
                        android:inputType="number"
                        android:text="2"
                        android:textAlignment="center"
                        android:textColor="@android:color/background_light"
                        android:textSize="24sp"
                        android:textStyle="bold"
                        android:typeface="sans" />


                </RelativeLayout>
            </android.support.v7.widget.CardView>

            <android.support.v7.widget.CardView
                android:id="@+id/CardView_2"
                android:layout_width="@dimen/CardViewWeight"
                android:layout_height="0dp"
                app:cardBackgroundColor="?attr/actionModeSplitBackground"
                app:layout_constraintBottom_toTopOf="@+id/line1"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintVertical_bias="0.666"
                tools:layout_editor_absoluteX="140dp">

                <RelativeLayout
                    android:layout_width="match_parent"
                    android:layout_height="match_parent">

                    <ImageView
                        android:id="@+id/imageView"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_alignParentStart="true"
                        android:layout_alignParentTop="true"
                        android:layout_marginStart="0dp"
                        android:background="@drawable/bg_ringgreen" />

                    <TextView
                        android:id="@+id/Assignment_2_text"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
